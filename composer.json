{
    "name": "valeryan/larainvite",
    "description": "Laravel Invitation package, existing users can invite others by email",
    "keywords": ["laravel", "invite", "invitations"],
    "homepage": "https://github.com/valeryan/larainvite",
    "license": "MIT",
    "authors": [
        {
            "name": "Junaid Nasir",
            "email": "contact@junaidnasir.com"
        }
    ],
    "require": {
        "php": ">=5.5.9",
        "illuminate/support": "^5.1",
        "illuminate/database": "^5.1",
        "illuminate/events": "^5.1",
        "illuminate/queue": "^5.1"
    },
    "autoload": {
        "psr-4": {
            "Valeryan\\Larainvite\\": "src"
        }
    },
    "autoload-dev": {
        "classmap": ["tests/TestCase.php"]   
    },
    "require-dev": {
        "phpunit/phpunit": "^5.2"
    }
}
